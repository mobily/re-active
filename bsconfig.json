{
  "name": "@mobily/re-active",
  "bsc-flags": [
    "-bs-no-version-header",
    "-bs-super-errors"
  ],
  "bs-dependencies": [
    "@mobily/bs-callbag",
    "reason-react"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest"
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "sources": [
    "src"
  ],
  "refmt": 3,
  "warnings": {
    "number": "+R",
    "error": "+R"
  }
}
